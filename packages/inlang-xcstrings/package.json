{
  "name": "inlang-plugin-xcstrings",
  "version": "0.0.1",
  "description": "An inlang plugin to handle Xcode String Catalogs",
  "keywords": [
    "inlang",
    "plugin",
    "xcstrings",
    "xcode",
    "translation",
    "ios",
    "mac"
  ],
  "license": "MIT",
  "author": "Lukas Hechenberger <npm@ls-age.com> (https://ls-age.com/)",
  "type": "module",
  "main": "./out/index.cjs",
  "module": "./out/index.js",
  "types": "./out/index.d.ts",
  "files": [
    "./out",
    "./marketplace-manifest.json",
    "./CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "inlang:build": "npx @inlang/cli module build --entry ./src/index.ts",
    "inlang:dev": "npx @inlang/cli module build --entry ./src/index.ts --watch",
    "prepublishOnly": "npm run -s sync-manifest",
    "sync-manifest": "node ./scripts/sync-manifests.mjs",
    "test": "vitest"
  },
  "devDependencies": {
    "@inlang/sdk": "^0.34.10",
    "@lix-js/client": "^1.4.0",
    "@sinclair/typebox": "^0.31.28",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
